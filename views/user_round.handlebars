<body>
   <div class="container">
      <h1 class="display-4 text-center course-header">User Rounds</h1>
   </div>
   <div class="container">
      <div class="row">
         <div class="col-md-12 text-center">
            <table class="table table-striped table-dark">
               <thead>
                  <tr>
                     <th scope="col">User Rounds Score</th>
                  </tr>
               </thead>
               <tbody>
                  <tr>
                     <td>    
                        {{#each usersRounds}}
                        {{score}}
                        {{/each}}
                     </td>
                  </tr>
               </tbody>
            </table>
         </div>
      </div>
   </div>
</body>